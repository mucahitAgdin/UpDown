{
  "name": "up-down",
  "version": "1.0.1",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.seninuygulaman.wol",
    "productName": "UpDown",
    "icon": "updown.ico",
    "files": [
      "src/**/*",
      "listener.exe",
      ".env"
    ],
    "extraResources": [
      {
        "from": "listener.exe",
        "to": "listener.exe"
      }
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "electron": "^35.2.0",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "ip": "^2.0.1",
    "ping": "^0.4.4",
    "sqlite3": "^5.1.7",
    "wake_on_lan": "^1.0.0",
    "wake-on-lan": "^0.1.0",
    "wol": "^1.0.7"
  }
}